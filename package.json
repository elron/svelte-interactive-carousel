{
	"name": "svelte-interactive-carousel",
	"description": "A smooth, draggable, stylable, responsive, friendly, interactive carousel that is basicly a marquee, but way cooler.",
	"version": "0.0.5",
	"scripts": {
		"dev": "vite dev",
		"publish": "",
		"package": "pnpm build && cd ./package && npm publish",
		"build:website": "vite build",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "1.0.0-next.76",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"svelte": "^3.50.0",
		"svelte-check": "^2.9.0",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2",
		"vite": "^3.1.0-beta.2"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"component",
		"interactive",
		"carousel",
		"marquee",
		"slide",
		"gallery",
		"draggable",
		"responsive",
		"ui",
		"autoplay",
		"autowidth",
		"pretty",
		"slick",
		"customizable",
		"touchable",
		"movable"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/elron/svelte-interactive-carousel.git"
	},
	"homepage": "https://svelte-interactive-carousel.netlify.app/",
	"bugs": {
		"url": "https://github.com/elron/svelte-interactive-carousel/issues"
	},
	"author": "Elron Bucai <its@elrons.co.il>",
	"license": "ISC"
}
